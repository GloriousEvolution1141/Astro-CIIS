---
import type Props from "../adapters/service";
const { title1, title2, description } = Astro.props;
---

<div class="w-52 p-4 text-center z-10">
    <h2 class="text-5xl font-bold mb-2.5 text-white">{title1}</h2>
    <div class="text-2xl font-bold mt-4 text-white">{title2}</div>
    <div class="text-sm mt-4">{description}</div>
</div>