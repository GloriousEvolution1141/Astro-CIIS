---
import LayoutRegister from "../../../../layouts/LayoutRegister.astro";
import InscriptionContainer from "../../../../views/inscription/InscriptionContainer.astro";
import PagoOptions from "../../../../views/inscription/components/PagoOptions";
import RedirectPlanNoValid from "../../../../views/inscription/components/RedirectPlanNoValid";

export function getStaticPaths() {
  return [
    { params: { tipoplan: "publicogeneral" } },
    { params: { tipoplan: "delegaciones" } },
    { params: { tipoplan: "docentesunjbg" } },
    { params: { tipoplan: "estudiantesunjbg" } },
    { params: { tipoplan: "estudiantesesis" } },
  ];
}

const { tipoplan } = Astro.params;
---

<LayoutRegister title="CIIS XXV">
  <InscriptionContainer>
    <RedirectPlanNoValid client:load tipoplan={tipoplan} />
    <PagoOptions client:load tipoplan={tipoplan} typeEvent="ciis" />
  </InscriptionContainer>
</LayoutRegister>
